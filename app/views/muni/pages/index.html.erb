<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Comuna</th>
      <th>Region</th>
      <th>Mayor</th>
      <th>Responsable</th>

      <th colspan="3"></th>
    </tr>
  </thead>



<tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.comuna %></td>
        <td><%= user.region %></td>
        <td><%= user.mayor %></td>
        <td><%= user.responsable %></td>
        <td><%= link_to "Profile", current_user %></td>
      </tr>
    <% end %>

