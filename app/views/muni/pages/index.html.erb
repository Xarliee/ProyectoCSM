<p id="notice"><%= notice %></p>

<h1>Listing Muni Pages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @muni_pages.each do |muni_page| %>
      <tr>
        <td><%= link_to 'Show', muni_page %></td>
        <td><%= link_to 'Edit', edit_muni_page_path(muni_page) %></td>
        <td><%= link_to 'Destroy', muni_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Page', new_muni_page_path %>
