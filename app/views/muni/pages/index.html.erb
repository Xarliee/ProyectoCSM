
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Region</th>
      <th>Mayor</th>
      <th>Responsable</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.region %></td>
        <td><%= user.mayor %></td>
        <td><%= user.responsable %></td>
        <td><%= user.role %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>
