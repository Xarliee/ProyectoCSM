<p id="notice"><%= notice %></p>

<h1>Listing Muni Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @muni_users.each do |muni_user| %>
      <tr>
        <td><%= link_to 'Show', muni_user %></td>
        <td><%= link_to 'Edit', edit_muni_user_path(muni_user) %></td>
        <td><%= link_to 'Destroy', muni_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_muni_user_path %>
