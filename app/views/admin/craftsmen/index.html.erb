<p id="notice"><%= notice %></p>

<h1>Listing Admin Craftsmen</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_craftsmen.each do |admin_craftsman| %>
      <tr>
        <td><%= link_to 'Show', admin_craftsman %></td>
        <td><%= link_to 'Edit', edit_admin_craftsman_path(admin_craftsman) %></td>
        <td><%= link_to 'Destroy', admin_craftsman, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Craftsman', new_admin_craftsman_path %>
